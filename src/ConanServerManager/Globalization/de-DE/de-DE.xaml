    <sys:String x:Key="ServerSettings_ServerDetailsLabel">Server Details</sys:String>

    <sys:String x:Key="ServerSettings_BranchLabel">Branch Details</sys:String>
    <sys:String x:Key="ServerSettings_BranchNameLabel">Branch Name:</sys:String>
    <sys:String x:Key="ServerSettings_BranchNameTooltip">Der Name des herunterzuladenden Conan Branch (leer lassen, um den Release Branch zu verwenden).</sys:String>
    <sys:String x:Key="ServerSettings_BranchPasswordLabel">Branch Password:</sys:String>
    <sys:String x:Key="ServerSettings_BranchPasswordTooltip">Das Kennwort, mit dem auf den Conan Branch Download zugegriffen wird.</sys:String>
    <sys:String x:Key="ServerSettings_BranchClearErrorTitle">Lösche Branch fehler</sys:String>
