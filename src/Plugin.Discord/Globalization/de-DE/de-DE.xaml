    <!--#region Global -->
    <sys:String x:Key="Global_CancelButtonLabel">Abbrechen</sys:String>
    <sys:String x:Key="Global_CloseButtonLabel">Schließen</sys:String>
    <sys:String x:Key="Global_OkButtonLabel">OK</sys:String>
    <sys:String x:Key="Global_SaveButtonLabel">Speichern</sys:String>

    <sys:String x:Key="Global_BetaModeLabel"> (Beta Modus)</sys:String>
    <sys:String x:Key="Global_NewVersionAvailableLabel">Eine neue Version des Plugins ist verfügbar.</sys:String>
    <sys:String x:Key="Global_DownloadNewVersionTooltip">Lade die neueste Version herunter.</sys:String>
